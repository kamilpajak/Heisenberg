# Heisenberg Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# =============================================================================
# AI Providers (at least one required for AI analysis)
# =============================================================================
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_API_KEY=

# =============================================================================
# GitHub (optional, for fetching artifacts from private repos)
# =============================================================================
GITHUB_TOKEN=

# =============================================================================
# Backend API (required for running the REST API server)
# =============================================================================
DATABASE_URL=postgresql://heisenberg:heisenberg@localhost:5432/heisenberg
SECRET_KEY=your-secret-key-change-in-production
LOG_LEVEL=INFO
LOG_JSON_FORMAT=false
